#include <iostream>
using namespace std;

class A {
private:
    int x;

public:
    A() {
        x = 100;
    }

    friend class B;
};

class B {
public:
    void display(A obj) {
        cout << "Value of x (private in A) = " << obj.x << endl;
    }
};

int main() {
    A objA;
    B objB;

    objB.display(objA);

    return 0;
}
